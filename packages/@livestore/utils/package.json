{
  "name": "@livestore/utils",
  "version": "0.2.0",
  "sideEffects": [
    "./dist/global.js"
  ],
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./cuid": {
      "types": "./dist/cuid/cuid.node.d.ts",
      "node": "./dist/cuid/cuid.node.js",
      "react-native": "./dist/cuid/cuid.browser.js",
      "browser": "./dist/cuid/cuid.browser.js"
    },
    "./nanoid": {
      "types": "./dist/nanoid/index.d.ts",
      "react-native": "./dist/nanoid/index.browser.js",
      "default": "./dist/nanoid/index.js"
    },
    "./effect": {
      "types": "./dist/effect/index.d.ts",
      "default": "./dist/effect/index.js"
    },
    "./node": {
      "types": "./dist/node/mod.d.ts",
      "default": "./dist/node/mod.js"
    },
    "./node-vitest": {
      "types": "./dist/node-vitest/mod.d.ts",
      "default": "./dist/node-vitest/mod.js"
    },
    "./node-vitest-polyfill": {
      "types": "./dist/node-vitest/polyfill.d.ts",
      "default": "./dist/node-vitest/polyfill.js"
    }
  },
  "react-native": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "./cuid": [
        "./dist/cuid/cuid.node.d.ts"
      ],
      "./nanoid": [
        "./dist/nanoid/index.d.ts"
      ],
      "./effect": [
        "./dist/effect/index.d.ts"
      ],
      "./node": [
        "./dist/node/mod.d.ts"
      ],
      "./node-vitest": [
        "./dist/node-vitest/mod.d.ts"
      ],
      "./node-vitest-polyfill": [
        "./dist/node-vitest/polyfill.d.ts"
      ]
    }
  },
  "scripts": {
    "test": "vitest"
  },
  "dependencies": {
    "@livestore/db-schema": "workspace:*",
    "msgpackr": "^1.11.2",
    "nanoid": "^5.0.7",
    "pretty-bytes": "^6.1.1"
  },
  "peerDependencies": {
    "@effect/cli": "^0.50.2",
    "@effect/experimental": "^0.34.2",
    "@effect/opentelemetry": "^0.41.5",
    "@effect/platform": "^0.71.2",
    "@effect/platform-browser": "^0.50.2",
    "@effect/platform-bun": "^0.51.2",
    "@effect/platform-node": "^0.66.2",
    "@effect/vitest": "^0.14.7",
    "@opentelemetry/api": "^1.9.0",
    "effect": "^3.11.7"
  },
  "devDependencies": {
    "@effect/cli": "^0.50.2",
    "@effect/experimental": "0.34.2",
    "@effect/opentelemetry": "0.41.5",
    "@effect/platform": "0.71.2",
    "@effect/platform-browser": "0.50.2",
    "@effect/platform-bun": "0.51.2",
    "@effect/platform-node": "^0.66.2",
    "@effect/vitest": "^0.14.7",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/exporter-metrics-otlp-http": "0.56.0",
    "@opentelemetry/exporter-trace-otlp-http": "0.56.0",
    "@opentelemetry/sdk-trace-node": "1.29.0",
    "@opentelemetry/sdk-metrics": "1.29.0",
    "@opentelemetry/sdk-trace-base": "1.29.0",
    "@types/node": "^22.10.0",
    "@types/web": "^0.0.175",
    "effect": "3.11.7",
    "vitest": "^2.1.4"
  },
  "publishConfig": {
    "access": "public"
  }
}